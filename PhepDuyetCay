void DuyetTruoc(Node A)
{
  XuLyTai(A); 
  Node B = TimNodeConTraiNhat(A);
  while (B != null)
  {
    DuyetTruoc(B);
    B = NhaySangNodePhai(B);
  }
}
void DuyetSau(Node A)
{
  Node B = TimNodeConTraiNhat(A);
  while (B != null) 
  {
    DuyetSau(B);
    B = NhaySangNodePhai(B);
  }
  XuLyTai(A);
}
void DuyetGiua(Node A)
{
  Node B = TimNodeConTraiNhat(A);
  while (B != null)
  {
    XuLyTai(DuyetGiua(B));
    B = NhaySangNodePhai(B);
  }
}
