using System;
public class Node
{
    public int data;
    public Node left;
    public Node right;
    public Node(int value)
    {
        data = value;
        left = null;
        right = null;
    }
    public void Insert(int value)
    {
        if (value >= data)
        {
            if (right == null)
                right = new Node(value);
            else
                right.Insert(value);
        }
        else
        {
            if (left == null)
                left = new Node(value);
            else
                left.Insert(value);
        }
    }
    public void PreOrder()
    {
        Console.Write(data + " ");
        if (left != null)
            left.PreOrder();
        if (right != null)
            right.PreOrder();
    }
    public void PostOrder()
    {
        if (left != null)
            left.PostOrder();
        if (right != null)
            right.PostOrder();
        Console.Write(data + " ");
    }
    public void InOrder()
    {
        if (left != null)
            left.InOrder();
        Console.Write(data + " ");
        if (right != null)
            right.InOrder();
    }
}
public class BinaryTree
{
    public Node root;
    public BinaryTree()
    {
        root = null;
    }
    public void Insert(int value)
    {
        if (root == null)
            root = new Node(value);
        else
            root.Insert(value);
    }
    public void PreOrder()
    {
        if (root != null)
            root.PreOrder();
    }
    public void PostOrder()
    {
        if (root != null)
            root.PostOrder();
    }
    public void InOrder()
    {
        if (root != null)
            root.InOrder();
    }
}
class Program
{
    static void Main(string[] args)
    {
        int[] A = { 75, 57, 90, 32, 7, 44, 60, 86, 93, 99, 100 };
        BinaryTree binaryTree = new BinaryTree();
        foreach (var i in A)
        {
            binaryTree.Insert(i);
        }
        Console.WriteLine("Phep Duyet Truoc");
        binaryTree.PreOrder();
        Console.WriteLine("\nPhep Duyet Sau");
        binaryTree.PostOrder();
        Console.WriteLine("\nPhep Duyet Giua");
        binaryTree.InOrder();
        Console.ReadLine();
    }
}
